{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "topographic-generator",
  "version-string": "0.22.2",
  "description": "High-performance topographic model generator with minimal dependencies",
  "dependencies": [
    "eigen3",
    "tbb",
    "cgal",
    {
      "name": "gdal",
      "default-features": false,
      "features": [
        "geotiff",
        "png"
      ]
    }
  ],
  "builtin-baseline": "master"
}
